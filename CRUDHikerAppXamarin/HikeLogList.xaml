<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viemodels ="clr-namespace:CRUDHikerAppXamarin.ViewModels"
             x:DataType="viemodels:HikeLogViewModel"
             x:Class="CRUDHikerAppXamarin.HikeLogList"
             Title="Hike Log List">
    <StackLayout Margin="15">
        <Label Text="Hike Log List" FontSize="30"/>
        <Button Text="Add Hike Log" HorizontalOptions="EndAndExpand" Command="{Binding AddUpdateHikeLogCommand}"></Button>
    </StackLayout>
</ContentPage>